#!/usr/bin/python
import matplotlib.pyplot as plt

def gcd(a,b):
     while b:
        a, b = b, a % b
     return a



x = []
y = []
k = 
i = 2
while(1):
    i+=1
    if (k<i): break
    if (gcd(k-i,i) == 1):
        x.append(k-i)
        y.append(i)

plt.plot(x, y, 'kx')

plt.xlabel('k')
plt.ylabel('s')
plt.title('5765234569876543567890876435678908745325678976544412')
plt.savefig('result.png')
