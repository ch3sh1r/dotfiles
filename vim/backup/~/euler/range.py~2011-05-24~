#!/usr/bin/python
import psyco ; psyco.full()

def euler(n):
     tot, pos = 0, n-1
     while pos>0:
        if euclid(pos,n)==1: tot += 1
        pos -= 1
     return tot

def euclid(a,b):
     while b:
        a, b = b, a % b
     return a


x = range(21)
y1 = [euler(2**i-1) for i in x]
y2 = [2**(i-1)-1 for i in x]

for i in x:
    print  '%d : %d, : %d,' % (i, y1[i], y2[i])
    if (y1[i] < y2[i]): print "FAIL"

