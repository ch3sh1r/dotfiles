#!/usr/bin/python
import psyco ; psyco.full()

def euler(n):
     tot, pos = 0, n-1
     while pos>0:
        if euclid(pos,n)==1: tot += 1
        pos -= 1
     return tot

def euclid(a,b):
     while b:
        a, b = b, a % b
     return a



for i in range(31):
    a = euler(2**i-1)
    b = 2**(i-1)-1
    print  '%3d : %s, : %s,' % (i,a,b)
    if (a < b): print "FAIL"

