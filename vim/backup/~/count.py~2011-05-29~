def totient(n):
    tot, pos = 0, n-1
    while pos>0:
        if gcd(pos,n)==1: tot += 1
        pos -= 1
    return tot

def gcd(a,b):
    while b:
       a, b = b, a % b
    return a

#for k in xrange(5,1000):
