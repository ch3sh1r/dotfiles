    #if (gcd(k-i,i) == 1): print "Number %d = %d + %d" % (k, k-i, i)

i=1
while(1):
    i+=1
    if (k<i): break
    if (gcd(k-i,i) == 1): print "Number %d = %d + %d" % (k, k-i, i)
